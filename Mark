local LibrarySource = game:HttpGet("https://raw.githubusercontent.com/deividcomsono/Obsidian/refs/heads/main/Library.lua", true)

local moveRight = 0
local moveLeft = 0
local moveDown = 0
local moveUp = 510

local offsetX = moveRight - moveLeft
local offsetY = moveDown - moveUp

LibrarySource = LibrarySource:gsub(
    'WatermarkBackground%.Position = UDim2%.fromOffset%(6, 6%)',
    'WatermarkBackground.Position = UDim2.new(0.5, '..offsetX..', 0.5, '..offsetY..'); WatermarkBackground.AnchorPoint = Vector2.new(0.5, 0.5)'
)

local Library = loadstring(LibrarySource)()
Library:Library:AddDraggableLabel(true)

local plr = game.Players.LocalPlayer

while task.wait() do
    local character = plr.Character
    if character and character:FindFirstChild("Humanoid") then
        local humanoid = character.Humanoid
        
        local isheld = plr.IsHeld.Value
        local inplot = plr.InPlot.Value

        local ragdoll = humanoid:FindFirstChild("Ragdolled") and humanoid.Ragdolled.Value or false
        local pd = humanoid:FindFirstChild("PaintDebounce") and humanoid.PaintDebounce.Value or false
        local fd = humanoid:FindFirstChild("FireDebounce") and humanoid.FireDebounce.Value or false

        local inpDP = inplot and "☑️" or "⬛"
        local hDP = isheld and "☑️" or "⬛"
        local rgdDP = ragdoll and "☑️" or "⬛"
        local fdPD = fd and "☑️" or "⬛"
        local pdPD = pd and "☑️" or "⬛"

        
		Library:SetWatermark(("InPlot = %s | IsHeld = %s | Ragdoll = %s | Burn = %s | Painted = %s"):format(
			inpDP,
            hDP,
			rgdDP,
            fdPD,
            pdPD
        ))
    else
        Library:SetWatermark("Loading character...")
    end
end
